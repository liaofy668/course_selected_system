<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linzhy.coursesystem.mapper.AdminMapper">
    <select id="login" resultType="com.linzhy.coursesystem.model.entity.Admin">
        select * from admin where admin_username = #{userName} and admin_password = #{password}
    </select>

</mapper>